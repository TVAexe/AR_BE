# Database Configuration (TiDB Cloud or Local MySQL)
SPRING_DATASOURCE_URL=jdbc:mysql://your-host:4000/your-database?sslMode=REQUIRED&enabledTLSProtocols=TLSv1.2,TLSv1.3
SPRING_DATASOURCE_USERNAME=your-username
SPRING_DATASOURCE_PASSWORD=your-password

# JWT Configuration
DEMO_JWT_BASE64_SECRET=your-base64-encoded-secret-key
DEMO_JWT_ACCESS_TOKEN_VALIDITY_IN_SECONDS=8640000000
DEMO_JWT_REFRESH_TOKEN_VALIDITY_IN_SECONDS=8640000000

# AWS S3 Configuration
AWS_S3_BUCKET_NAME=your-bucket-name
AWS_S3_REGION=ap-southeast-1
AWS_S3_ACCESS_KEY=your-aws-access-key
AWS_S3_SECRET_KEY=your-aws-secret-key

# Server Configuration
SERVER_PORT=8080

# JPA Configuration
SPRING_JPA_HIBERNATE_DDL_AUTO=update
SPRING_JPA_SHOW_SQL=false